{
  "elasticsearchHost": "{{ Elasticsearch host address }}",
  "index": "{{ index name }}",
  "query": {
    "bool": {
			"must": [
				{
					"query_string": {
						"query": "metadata.confidential:false AND ((metadata.sensitivity_blur:B) OR (!metadata.sensitivity_blur:*)) AND _exists_:taxon.taxon_id",
						"analyze_wildcard": true,
						"default_field": "*"
					}
				},
				{
					"term": {"metadata.website.id": 2}
				},
				{
					"term": {"identification.verification_status": "V"}
				},
				{
					"term": {"metadata.release_status": "R"}
				}
			]
    }
  },
  "outputType": "dwca",
  "options": [
	  "useGridRefsIfPossible"
  ],
  "occurrenceIdPrefix": "",
  "defaultLicenceCode": "",
  "datasetIdSampleAttrId": 123,
  "basisOfRecord": "HumanObservation",
  "xmlFilesInDir": "metadata/export-example-occurrence",
	"repeatExport": [
		{
			"outputFile": "exports/survey-2.zip",
			"rightsHolder": "Test rights holder 1",
  		"datasetName": "Test dataset name 1",
			"surveyId": 2,
			"higherGeographyId": 1
		},
		{
			"outputFile": "exports/survey-3.zip",
			"rightsHolder": "Test rights holder 2",
  		"datasetName": "Test dataset name 2",
			"surveyId": 2,
			"higherGeographyId": 2
		}
	]
}